        <select id="getFoodList" resultType="com.kgitbank.model.Foods">
           SELECT * FROM foods
        </select>
        

        
        <insert id="create">
        	INSERT INTO employees(employee_id, last_name, email, hire_date, job_id) VALUES
        	(employees_seq.nextval, #{last_name}, #{email}, #{hire_date}, #{job_id})
        </insert>
        
</mapper>